package com.in28minutes.oops;

public class IfStatementRunner {

	public static void main(String[] args) {

		//int i=25;
		int i=24;
		
		if(i == 25) {
		System.out.println("i = 25");
		} else if(i == 24) {
		System.out.println("i = 24");
		} else {
		System.out.println("i != 25 and i != 24");
		}


	}

}
